}
.journey-container{
   display:flex;
   flex-direction:row;
    border: 1px solid #405f80;
    width: 400rpx;
    height: 100rpx;
    border-radius: 10px;
   justify-content:center;
    align-items: center;
    margin-top: 30rpx;
    margin-left: 176rpx;
    background: #405f80;
}

.journey{
    font-size:22rpx;
